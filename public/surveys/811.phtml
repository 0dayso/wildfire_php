	if(_pageIndex == pageNum) {
		Element.show($('submit'));
	} else if ( _pageIndex &lt; pageNum) {
		Element.hide($('submit'));
	}
  }
  
  function showNextPage(){
  	showPage(currentPage+1);
  }
  
  function showPrevPage(){
  	showPage(currentPage-1);
  }
  
  function initPages() {
  	var i;

  	for (i=0; i&lt;=pageNum; i++) {
